{
    "name": "dutch-dynamics",
    "version": "1.0.0",
    "description": "De discord bot achter discord dynamics",
    "main": "dist/index.js",
    "engines": {
        "node": ">=20.15.1"
    },
    "scripts": {
        "lint": "gts lint",
        "fix": "gts fix",
        "build": "gts clean && tsc --project tsconfig.json && tsc-alias",
        "start": "node dist/index.js",
        "dev": "nodemon --legacy-watch --watch \"src/**\" --ext \"ts\" --exec \"ts-node -r tsconfig-paths/register ./src/index.ts\"",
        "dev:enter:bot": "docker exec -it discord-dynamics-bot-1 bash",
        "dev:enter:mariadb": "docker exec -it discord-dynamics-mariadb-1 mariadb --user=prisma --password=password discord-bot",
        "new:task": "ts-node scripts/task.ts",
        "new:event": "ts-node scripts/event.ts",
        "new:interaction": "ts-node scripts/interaction.ts"
    },
    "author": "Jarco-dev",
    "dependencies": {
        "@discordjs/builders": "1.8.x",
        "@prisma/client": "^5.7.0",
        "@sematext/gc-stats": "^1.5.9",
        "discord.js": "14.15.3",
        "dotenv": "^16.3.1",
        "express": "^4.21.2",
        "moment": "^2.29.4",
        "prom-client": "^15.1.3"
    },
    "devDependencies": {
        "@types/express": "^5.0.0",
        "@types/inquirer": "^8.2.5",
        "@types/node": "16.9.x",
        "@types/node-cron": "^3.0.11",
        "@types/sematext__gc-stats": "^1.5.3",
        "cron-validator": "^1.3.1",
        "gts": "^5.2.0",
        "handlebars": "^4.7.8",
        "inquirer": "^8.2.5",
        "node-cron": "^3.0.3",
        "nodemon": "^3.0.2",
        "prisma": "^5.7.0",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.8",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.3.3"
    }
}
